<!--components/start/star.wxml-->
<view class="star-container">
  <view class="stars">
    <block wx:for="{{stars}}" wx:key="index">
       <image wx:if="{{item == 1}}"  class="star" src="/images/icon/star.png"></image>
      <image wx:else="{{item == 0}}"  class="star" src="/images/icon/none-star.png"></image> 
    </block>
  </view>
  <text class="star-score">{{score}}</text>
</view>